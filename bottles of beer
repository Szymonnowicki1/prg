#include <iostream>

using namespace std;

int i = 99;
int main()
{ for(int i = 99 ;i>0 ;i--)
{


    cout << i <<" bottles of beer on the wall" << i << " bottles of beer" <<endl;
    cout << " take one down, pass it around," << i-1 << " bottles of beer on the wall" << endl;
}
    cout << "no more bottles of beer on the wall, no more bottles of beer" <<endl;
    cout << "go to the store and buy some more, " << i << " bottles of beer on the wall" <<endl;
}
